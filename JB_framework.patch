+    //        Log.d(TAG, "createDatabase - closing connection to privacy.db");
+            if (db != null && db.isOpen()) db.close();
+        } catch (SQLException e) {
+            Log.e(TAG, "createDatabase - failed to create privacy database", e);
+        }
+    }
